#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules/.pnpm/esbuild@0.14.38/node_modules/esbuild/bin/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules/.pnpm/esbuild@0.14.38/node_modules/esbuild/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules/.pnpm/esbuild@0.14.38/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules/.pnpm/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/node_modules:/Users/iarroyo/Development/projects/experiments/ember/node_modules:/Users/iarroyo/Development/projects/experiments/node_modules:/Users/iarroyo/Development/projects/node_modules:/Users/iarroyo/Development/node_modules:/Users/iarroyo/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules/.pnpm/esbuild@0.14.38/node_modules/esbuild/bin/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules/.pnpm/esbuild@0.14.38/node_modules/esbuild/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules/.pnpm/esbuild@0.14.38/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules/.pnpm/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/node_modules:/Users/iarroyo/Development/projects/experiments/ember/node_modules:/Users/iarroyo/Development/projects/experiments/node_modules:/Users/iarroyo/Development/projects/node_modules:/Users/iarroyo/Development/node_modules:/Users/iarroyo/node_modules:/Users/node_modules:/node_modules"
fi
"$basedir/../../bin/esbuild"   "$@"
exit $?
