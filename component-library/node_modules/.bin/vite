#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules/.pnpm/vite@2.9.5/node_modules/vite/bin/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules/.pnpm/vite@2.9.5/node_modules/vite/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules/.pnpm/vite@2.9.5/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules/.pnpm/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/node_modules:/Users/iarroyo/Development/projects/experiments/ember/node_modules:/Users/iarroyo/Development/projects/experiments/node_modules:/Users/iarroyo/Development/projects/node_modules:/Users/iarroyo/Development/node_modules:/Users/iarroyo/node_modules:/Users/node_modules:/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules/vite/bin/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules/vite/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules/.pnpm/vite@2.9.5/node_modules/vite/bin/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules/.pnpm/vite@2.9.5/node_modules/vite/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules/.pnpm/vite@2.9.5/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules/.pnpm/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/node_modules:/Users/iarroyo/Development/projects/experiments/ember/node_modules:/Users/iarroyo/Development/projects/experiments/node_modules:/Users/iarroyo/Development/projects/node_modules:/Users/iarroyo/Development/node_modules:/Users/iarroyo/node_modules:/Users/node_modules:/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules/vite/bin/node_modules:/Users/iarroyo/Development/projects/experiments/ember/ember-lit-poc/component-library/node_modules/vite/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
